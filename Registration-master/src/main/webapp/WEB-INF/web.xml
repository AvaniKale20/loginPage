  	<servlet-class>com.bridgelabz.controller.dfsdgf</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ForgetPassword</servlet-name>
  	<display-name>ForgetPassword</display-name>
  	<description></description>
  	<servlet-class>com.bridgelabz.controller.ForgetPassword</servlet-class>
  </servlet>
  <servlet>
